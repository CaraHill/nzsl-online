= render partial: 'pages/content'
%ul.small-11.small-centered.medium-10.large-8.no-bullet
  - alphabet_listing.each_with_index do |letter, i|
    %li.small-6.medium-4.large-3.grid-stack.float-left.text-left
      %div
        = image_tag "alphabet/82/Alphabet.#{letter}.png", alt: letter.humanize, class: "grid-image"
        %div.grid-image-caption
          = letter.humanize
